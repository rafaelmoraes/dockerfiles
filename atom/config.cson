"*":
  core:
    autoHideMenuBar: true
    automaticallyUpdate: false
    disabledPackages: [
      "metrics"
    ]
    themes: [
      "one-light-ui"
      "solarized-light-syntax"
    ]
  editor:
    fontFamily: "Roboto Mono for Powerline, Menlo, Consolas, DejaVu Sans Mono, monospace"
    lineHeight: 1.3
    showIndentGuide: true
  welcome:
    showOnStartup: false
